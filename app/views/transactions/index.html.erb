<%= link_to root_path, class: "p-4" do %>
  <i class="fa fa-arrow-left"></i>
<% end %>

<h1>Transactions</h1>

<table>
  <% @transactions.each do |transaction| %>
    <tr>
      <td><%= transaction.id %></li>
      <td><%= transaction.symbol %></li>
      <td><%= transaction.transaction_type %></li>
      <td><%= transaction.price %></li>
      <td><%= transaction.quantity %></li>
      <td><%= transaction.currency %></li>
    </tr>
  <% end %>
</table>